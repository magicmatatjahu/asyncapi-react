{
  "name": "@asyncapi/editor",
  "version": "1.0.0-next.18",
  "description": "AsyncAPI editor // change the description",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asyncapi/asyncapi-react"
  },
  "author": {
    "name": "The AsyncAPI maintainers",
    "url": "https://www.asyncapi.com"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/asyncapi/asyncapi-react/issues"
  },
  "keywords": [
    "editor",
    "asyncapi",
    "asyncapi-specification",
    "react",
    "reactjs",
    "event"
  ],
  "tags": [
    "editor",
    "asyncapi",
    "asyncapi-specification",
    "react",
    "reactjs",
    "event"
  ],
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "files": [
    "/lib",
    "/browser",
    "/styles",
    "./README.md",
    "./LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "start": "concurrently \"tsc -p tsconfig.esm.json --watch\" \"tsc -p tsconfig.types.json --watch\"",
    "build:dev": "npm run build:esm && npm run build:types",
    "build:prod": "npm run build:esm && npm run build:cjs && npm run build:types",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:types": "tsc -p tsconfig.types.json",
    "build:workers": "webpack --mode production --colors",
    "prepare": "npm run build:dev",
    "install:reactcomp": "chmod +x ./bump-react-comp.sh && ./bump-react-comp.sh"
  },
  "dependencies": {
    "@asyncapi/converter": "^0.5.0",
    "@asyncapi/react-component": "^1.0.0-next.8",
    "@hookstate/core": "^3.0.8",
    "@monaco-editor/react": "^4.2.0",
    "@headlessui/react": "1.4.0",
    "react-hot-toast": "2.1.1",
    "js-file-download": "^0.4.12",
    "js-yaml": "^4.1.0",
    "monaco-editor": "^0.25.2",
    "monaco-yaml": "^2.5.1",
    "react-icons": "^4.2.0",
    "react-keep-alive": "^2.5.2",
    "react-reflex": "^4.0.1",
    "react-split-pane": "^0.1.92"
  },
  "devDependencies": {
    "@types/jest": "26.0.23",
    "@types/js-yaml": "^4.0.2",
    "@types/node": "^15.12.2",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "typescript": "^4.3.2",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^3.3.12"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
